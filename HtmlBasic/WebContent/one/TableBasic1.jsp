<title>Insert title here</title>
</head>

<body>
	
<!-- 	표를 만드는 태그다 -->
<!-- 	table 태그는 행과 열을 가진다 -->
<!-- 	행은 tr -->
<!-- 	열은 td로 만든다 -->
<!-- 	td태그는 셀(Cell)이라고도 한다 -->
	
<!-- 	기본구조 -->
<!-- 	<table> -->
<!-- 		<tr> -->
<!-- 			<td>내용</td> -->
<!-- 		</tr> -->
<!-- 	</table> -->
<!-- 		border -> 테두리를 만들어준다  -->
	<table border="1px">
		<tr>
			<td>1행1열</td>
		</tr>
	</table>
	
	<br/>
	
	<table border="1px">
		<tr>
			<td>1행1열</td>
			<td>1행2열</td>
		</tr>
	</table>
	
	<br/>
	
	<table border="1px">
		<tr>
			<td>1행1열</td>
		</tr>
		<tr>
			<td>2행1열</td>
		</tr>
	</table>
	
	<br/>
	
	<table border="1px">
		<tr>
			<td>1행1열</td><td>1행2열</td>
		</tr>
		<tr>
			<td>2행1열</td><td>2행2열</td>
		</tr>
	</table>
	
	<br/>
	
	
	

</body>
</html>